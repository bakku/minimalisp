(defun say-hello (name) (println name))
(defvar first "Christian")
(defun print-with-last (last) (println first last))
(defun print-name (first last long-format)
  (if long-format
    (println first last)
    (println "Mr./Mrs." last)))
	
(say-hello "Steven")
(print-with-last "Miller")
(print-name first "Miller" false)
